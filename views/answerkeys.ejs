<% include partials/header %>

<div class="jumbotron notebookpic">
    <h1 id="pagetitle">SAT Math Problem Answers</h1>
</div>
<div class="container pagetext">
    <table class="table">
       <thead>
           <tr>
               <th scope="col">Test Number</th>
               <th scope="col">Question Number</th>
               <th scope="col">Question Image</th>
               <th scope="col">Answer</th>
               <th scope="col">Category</th>
           </tr>
       </thead>
        <% questions.forEach(function(question){ %>
            <tr>
                <td><%= question.test_num%></td>
                <td><%= question.question_num%></td>
                <% var imageID = question.image_link.match(/\/d\/(.+?)\/view/)[1]; %>
                <% var imageURL = "https://drive.google.com/uc?export=view&id=" + imageID %>
                <td><img src="<%= imageURL %>" width="600px"></td>
                <td><%= question.answer %></td>
                <td><%= question.category %></td>
            </tr>
        <% }) %>
    </table>
</div>

<% include partials/footer %>
